{
  "compilerOptions": {
    // "baseUrl": ".",
    // "paths": {
    //   "@/*": ["src/*"] // remove because it got some error
    // },
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true, 
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}